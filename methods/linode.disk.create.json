{
  "name": "linode.disk.create()",
  "params": {
    "LinodeID": {
      "type": "numeric",
      "required": true,
      "description": ""
    },
    "FromDistributionID": {
      "type": "numeric",
      "required": false,
      "description": ""
    },
    "rootPass": {
      "type": "string",
      "required": false,
      "description": ""
    },
    "rootSSHKey": {
      "type": "string",
      "required": false,
      "description": ""
    },
    "Label": {
      "type": "string",
      "required": true,
      "description": "The display label for this Disk"
    },
    "Type": {
      "type": "string",
      "required": true,
      "description": "The formatted type of this disk.  Valid types are: ext3, ext4, swap, raw"
    },
    "isReadOnly": {
      "type": "boolean",
      "required": false,
      "description": "Enable forced read-only for this Disk"
    },
    "Size": {
      "type": "numeric",
      "required": true,
      "description": "The size in MB of this Disk."
    }
  }
}