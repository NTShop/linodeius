      "type": "numeric",
      "required": false,
      "description": ""
    },
    "backupWeeklyDay": {
      "type": "numeric",
      "required": false,
      "description": ""
    },
    "watchdog": {
      "type": "boolean",
      "required": false,
      "description": "Enable the Lassie shutdown watchdog"
    },
    "ms_ssh_disabled": {
      "type": "boolean",
      "required": false,
      "description": ""
    },
    "ms_ssh_user": {
      "type": "string",
      "required": false,
      "description": ""
    },
    "ms_ssh_ip": {
      "type": "string",
      "required": false,
      "description": ""
    },
    "ms_ssh_port": {
      "type": "numeric",
      "required": false,
      "description": ""
    }
