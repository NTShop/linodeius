{
  "name": "linode.update()",
  "params": {
    "LinodeID": {
      "type": "numeric",
      "required": true,
      "description": ""
    },
    "Label": {
      "type": "string",
      "required": false,
      "description": "This Linode's label"
    },
    "lpm_displayGroup": {
      "type": "string",
      "required": false,
      "description": "Display group in the Linode list inside the Linode Manager"
    },
    "Alert_cpu_enabled": {
      "type": "boolean",
      "required": false,
      "description": "Enable the cpu usage email alert"
    },
    "Alert_cpu_threshold": {
      "type": "numeric",
      "required": false,
      "description": "CPU Alert threshold, percentage 0-800"
    },
    "Alert_diskio_enabled": {
      "type": "boolean",
      "required": false,
      "description": "Enable the disk IO email alert"
    },
    "Alert_diskio_threshold": {
      "type": "numeric",
      "required": false,
      "description": "IO ops/sec"
    },
    "Alert_bwin_enabled": {
      "type": "boolean",
      "required": false,
      "description": "Enable the incoming bandwidth email alert"
    },
    "Alert_bwin_threshold": {
      "type": "numeric",
      "required": false,
      "description": "Mb/sec"
    },
    "Alert_bwout_enabled": {
      "type": "boolean",
      "required": false,
      "description": "Enable the outgoing bandwidth email alert"
    },
    "Alert_bwout_threshold": {
      "type": "numeric",
      "required": false,
      "description": "Mb/sec"
    },
    "Alert_bwquota_enabled": {
      "type": "boolean",
      "required": false,
      "description": "Enable the bw quote email alert"
